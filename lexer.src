/* Reserved words list */
"part"							{ return PART; }
"bit"								{ return BIT; }

/* Other token types */
[0-9][0-9]*						{ return NUM; }
[a-zA-Z][_a-zA-Z0-9]*		{ return IDENT; }

[;]								{ return *yytext; }
[ \t\n]							/* Consume all whitespace */



%%
